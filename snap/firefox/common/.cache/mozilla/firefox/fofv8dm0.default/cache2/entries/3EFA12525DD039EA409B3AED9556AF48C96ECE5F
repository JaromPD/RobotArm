<script>
    function gotoHomepage(url){
        location.href = url;
    }
    function searchOpen(){
        $("#stx2").val('');
        if($('#respMenu').css('display') == 'block'){
            $('#menu-btn').click();
        }
        $('#popup_search').show();
        $('#stx2').focus();
    }
    function searchClose(){
        $("#stx2").val('');
        $("#popup_search").hide();
    }
</script>


