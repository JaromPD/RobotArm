    function send_main_email() {
        let email_name = $('#email_name').val();
        let email_email = $('#email_email').val();
        let email_context = $('#email_context').val();
        let mail_token = $('#mail_token').val();

        if(email_name == '') {
            alert('ì´ë¦„ì„ ì…ë ¥í•´ì£¼ì„¸ìš”.');
            return false;
        }

        if(email_email == '') {
            alert('ë©”ì¼ì„ ì…ë ¥í•´ì£¼ì„¸ìš”.');
            return false;
        }

        if(email_context == '') {
            alert('ë‚´ìš©ì„ ì…ë ¥í•´ì£¼ì„¸ìš”.');
            return false;
        }

        let ajax_url = "./ajax.mail_send.php";
        let ajax_data = "email_name=" + email_name;
        ajax_data += "&email_email=" + email_email;
        ajax_data += "&email_context=" + email_context;
        ajax_data += "&mail_token=" + mail_token;

        $("#mailBtn").hide();
        $("#mailIng").show();

        $.ajax({
            type: "POST",
            url: ajax_url,
            data : ajax_data,
            dataType: 'jsonp',
            jsonpCallback: "callback",
            success: function (jsonData) {
                console.log(jsonData);
                if(jsonData.code=="000") {
                    alert('ë©”ì¼ì„ ë³´ëƒˆìŠµë‹ˆë‹¤.');
                } else if(jsonData.code == "100") {
                    alert('í† í° ì •ë³´ê°€ ì˜¬ë°”ë¥´ì§€ ì•ŠìŠµë‹ˆë‹¤. ìƒˆë¡œê³ ì¹¨ í›„ ë‹¤ì‹œ ì‹œë„í•´ì£¼ì„¸ìš”.');
                } else if(jsonData.code == "300") {
                    alert('ì´ë©”ì¼ ì£¼ì†Œë¥¼ í™•ì¸í•´ì£¼ì„¸ìš”.');
                }else{
                    alert('ì ìš©ì‹¤íŒ¨');
                }
                $("#mailBtn").show();
                $("#mailIng").hide();
            },
            error:function(request, status, error){
                console.log(error);
                alert('ë©”ì¼ ë°œì†¡ì— ì‹¤íŒ¨í–ˆìŠµë‹ˆë‹¤.');
            }
        });
    }

    export function detectWebpSupport() {
        const image = new Image();
        const webpDataURL = "data:image/webp;base64,UklGRiQAAABXRUJQVlA4IBgAAAAwAQCdASoBAAEAAwA0JaQAA3AA/vuUAAA=";

        const callback = (event) => {
            const isWebPSupported = event?.type === "load" && image.width === 1;

            if (isWebPSupported) {
                document.body.classList.add("webp");
                document.body.classList.remove("no-webp");
            } else {
                document.body.classList.add("no-webp");
                document.body.classList.remove("webp");
            }
        };

        image.onerror = callback;
        image.onload = callback;
        image.src = webpDataURL;
    }

    // í˜¸ì¶œí•˜ì—¬ WebP ì§€ì› ì—¬ë¶€ë¥¼ ê°ì§€
    detectWebpSupport();
</script>
